    <definition>&lt;def-root&gt;Subframe of Transfer in which the &lt;fen&gt;Donor&lt;/fen&gt; begins by possessing a &lt;fen&gt;Theme&lt;/fen&gt; and finishes by not possessing the &lt;fen&gt;Theme&lt;/fen&gt;.&lt;/def-root&gt;</definition>
    <semType name="Non-Lexical Frame" ID="16"/>
    <FE bgColor="008000" fgColor="FFFFFF" coreType="Peripheral" cBy="MJE" cDate="08/20/2001 05:05:06 PDT Mon" abbrev="Place" name="Place" ID="1672">
        <definition>&lt;def-root&gt;The &lt;fen&gt;Place&lt;/fen&gt; is where the Relinquishing occurs.&lt;/def-root&gt;</definition>
        <semType name="Locative_relation" ID="182"/>
    </FE>
    <FE bgColor="00BFFF" fgColor="FFFFFF" coreType="Extra-Thematic" cBy="MJE" cDate="08/20/2001 05:05:06 PDT Mon" abbrev="Exp" name="Explanation" ID="1673">
        <definition>&lt;def-root&gt;The &lt;fen&gt;Explanation&lt;/fen&gt; indicates what the &lt;fen&gt;Donor&lt;/fen&gt; is trying to accomplish by the Relinquishing.&lt;/def-root&gt;</definition>
        <semType name="State_of_affairs" ID="177"/>
    </FE>
    <FE bgColor="FFA500" fgColor="FFFFFF" coreType="Peripheral" cBy="MJE" cDate="08/20/2001 05:05:06 PDT Mon" abbrev="Time" name="Time" ID="1674">
        <definition>&lt;def-root&gt;The &lt;fen&gt;Time&lt;/fen&gt; indicates when the Relinquishing occurs.&lt;/def-root&gt;</definition>
        <semType name="Time" ID="141"/>
    </FE>
    <FE bgColor="0000FF" fgColor="FFFFFF" coreType="Core" cBy="MJE" cDate="08/20/2001 05:05:24 PDT Mon" abbrev="Donor" name="Donor" ID="1675">
        <definition>&lt;def-root&gt;The person that begins in possession of the &lt;fen&gt;Theme&lt;/fen&gt; and causes it to be in the possession of the &lt;fen&gt;Recipient&lt;/fen&gt;.&lt;/def-root&gt;</definition>
    </FE>
    <FE bgColor="800080" fgColor="FFFFFF" coreType="Core" cBy="MJE" cDate="08/20/2001 05:05:27 PDT Mon" abbrev="Thm" name="Theme" ID="1676">
        <definition>&lt;def-root&gt;The &lt;fen&gt;Theme&lt;/fen&gt; is the object that changes ownership due to the Relinquishing.&lt;/def-root&gt;</definition>
        <semType name="Physical_object" ID="68"/>
    </FE>
    <FE bgColor="9F79EE" fgColor="FFFFFF" coreType="Extra-Thematic" cBy="MJE" cDate="08/21/2001 08:22:28 PDT Tue" abbrev="Purp" name="Purpose" ID="1717">
        <definition>&lt;def-root&gt;The &lt;fen&gt;Purpose&lt;/fen&gt; indicates the purpose for which the intentional Relinquishing takes place.&lt;/def-root&gt;</definition>
        <semType name="State_of_affairs" ID="177"/>
    </FE>
    <FE bgColor="00008B" fgColor="FFFFFF" coreType="Core" cBy="MJE" cDate="08/21/2001 08:22:28 PDT Tue" abbrev="Rec" name="Recipient" ID="1718">
        <definition>&lt;def-root&gt;The &lt;fen&gt;Recipient&lt;/fen&gt; is the entity that ends up in possession of the &lt;fen&gt;Theme&lt;/fen&gt;.&lt;/def-root&gt;</definition>
    </FE>
