        <semType name="Time" ID="141"/>
    </FE>
    <FE bgColor="FF69B4" fgColor="FFFFFF" coreType="Peripheral" cBy="JKR" cDate="06/29/2004 06:49:44 PDT Tue" abbrev="Degr" name="Degree" ID="6643">
