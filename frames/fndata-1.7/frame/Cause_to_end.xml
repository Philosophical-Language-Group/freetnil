        <semType name="State_of_affairs" ID="177"/>
    </FE>
    <FE bgColor="FFA500" fgColor="FFFFFF" coreType="Peripheral" cBy="JKR" cDate="06/29/2004 06:49:43 PDT Tue" abbrev="Time" name="Time" ID="6642">
